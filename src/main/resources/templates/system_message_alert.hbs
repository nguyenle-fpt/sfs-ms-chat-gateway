<messageML>
  <div>
    <card data-accent-color="tempo-bg-color--red"
          data-icon-src="data:image/png;base64,">
      {{#if message.title}}
        <header>
          <b>{{message.title}}</b>
        </header>
      {{/if}}

      {{#if message.content}}
        <body>
        {{message.content}}
        </body>
      {{/if}}
    </card>
  </div>
</messageML>
